{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false,
    "outDir": ".vercel/output/functions"
  },
  "include": ["api/**/*.ts"],
  "exclude": ["node_modules"]
}
